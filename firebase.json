{
  "apphosting": {
    "backendId": "yarus-shop",
    "rootDir": ".",
    "ignore": [
      "node_modules",
      ".git",
      ".next",
      ".vscode",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ]
  },
  "emulators": {
    "apphosting": {
      "port": 5002,
      "rootDirectory": "./",
      "startCommand": "next dev --turbo"
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  }
}
